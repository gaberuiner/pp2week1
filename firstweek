task 8:
a = input()
b = input()
if a.count(b) == 1:
    print(a.find(b))
else:
    if a.count(b) >= 2:
        print(a.find(b), a.rfind(b))



task 9:
a = int(input())
for i in range(0, a):
    b = input()
    if b.rfind('@gmail.com') == -1:
        b = b
    else:
        c = len(b) - 10
        
        d = b.rfind('@gmail.com')
        if c == d:
            b = b.replace('@gmail.com','')
            print(b)



task 10:
a = input()
b = a.split()
for element in b:
    if len(element) >= 3:
        print(element, end = ' ')
        
