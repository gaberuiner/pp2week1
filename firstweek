task 4
a = float(input())
b = input()
if b == 'k':
    c = int(input())
    a = a/1024
    print(round(a, c))
if b == 'b':
    print(round(a*1024))
task 5:
n = list(map(int,input().split(' ')))

a = True
c = 0
for i in range(1, n[0]+1):
    if(n[0]%i == 0):
        c = c + 1
if(c == 2):
    a = True
else:
    a = False
if n[0] <500 and a == True and (n[1]%2 == 0):
    print('Good job!')
else:
    print('Try next time!')
    
    
    
    

task 6:
a = int(input())
for i in range (1, a+1):
    b = int(input())
    if b <= 10:
        print('Go to work!')
    else:
        if 10 < b <= 25:
            print('You are weak')
        else:
            if 25 < b <= 45:
                print('Okay, fine')
            else:
                print('Burn! Burn! Burn Young!')
                
                
                
                
                
